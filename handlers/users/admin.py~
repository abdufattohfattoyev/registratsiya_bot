    await AdminStates.waiting_for_event_language.set()


# KANALLAR BOSHQARUVI

async def channels_management_handler(message: types.Message):
    """Kanallar boshqaruvi"""
    if not is_admin(message.from_user.id):
        await message.answer("âŒ Bu komanda faqat adminlar uchun!")
        return

    try:
        db = Database()
        channels = db.get_all_channels()

        channels_text = "ğŸ“¢ <b>KANALLAR BOSHQARUVI</b>\n\n"

        if not channels:
            channels_text += "âŒ Hozirda kanallar yo'q.\n\n"
        else:
            for channel_id, channel_name, channel_username, channel_type in channels:
                channels_text += f"ğŸ“¢ <b>{channel_name}</b>\n"
                channels_text += f"ğŸ†” ID: <code>{channel_id}</code>\n"
                channels_text += f"ğŸ”— Username: @{channel_username or 'yoq'}\n"
                channels_text += f"ğŸ“Š Turi: {channel_type}\n\n"

        keyboard = types.InlineKeyboardMarkup(row_width=1)
        keyboard.add(types.InlineKeyboardButton("â• Kanal qo'shish", callback_data="add_channel"))

        if channels:
            for channel_id, channel_name, channel_username, channel_type in channels:
                channel_name_short = channel_name[:15] + "..." if len(channel_name) > 15 else channel_name
                keyboard.add(types.InlineKeyboardButton(
                    f"âŒ {channel_name_short} ni o'chirish",
                    callback_data=f"remove_channel_{channel_id}"
                ))

        await message.answer(channels_text, reply_markup=keyboard, parse_mode='HTML')

    except Exception as e:
        await message.answer(f"âŒ Xatolik: {e}")


async def add_channel_callback(callback_query: types.CallbackQuery):
    """Kanal qo'shish callback"""
    if not is_admin(callback_query.from_user.id):
        await callback_query.answer("âŒ Ruxsat yo'q!")
        return

    help_text = (
        "ğŸ“¢ <b>Kanal qo'shish</b>\n\n"
        "ğŸ”— <b>Qo'llab-quvvatlanadigan formatlar:</b>\n"
        "â€¢ <code>@kanal_nomi</code>\n"
        "â€¢ <code>kanal_nomi</code>\n"
        "â€¢ <code>t.me/kanal_nomi</code>\n"
        "â€¢ <code>-100123456789</code> (ID)\n\n"
        "âŒ <b>Qo'llab-quvvatlanmaydigan:</b>\n"
        "â€¢ <code>t.me/joinchat/...</code>\n"
        "â€¢ <code>t.me/+...</code>\n"
        "â€¢ <code>https://t.me/public</code>\n\n"
        "ğŸ’¡ <b>Maslahat:</b> Kanal username yoki ID dan foydalaning!\n\n"
        "ğŸ‘‡ <b>Kanal link yoki ID sini yuboring:</b>"
    )

    await callback_query.message.answer(help_text, parse_mode='HTML')
    await AdminStates.waiting_for_channel_input.set()
    await callback_query.answer()


async def process_channel_input(message: types.Message, state: FSMContext):
    """Improved channel input processing with better error handling"""
    if not is_admin(message.from_user.id):
        await message.answer("âŒ Access denied!")
        await state.finish()
        return

    channel_input = message.text.strip()

    if not channel_input:
        await message.answer("âŒ Please provide a channel link or ID!")
        return

    try:
        # Show loading message
        loading_msg = await message.answer("ğŸ”„ Checking channel...")

        db = Database()
        # CRITICAL: Ensure this is awaited properly
        result = await db.add_channel_smart(message.bot, channel_input)

        # Clean up loading message
        try:
            await loading_msg.delete()
        except:
            pass

        # Send result
        await message.answer(
            result['message'],
            reply_markup=get_admin_keyboard(),
            parse_mode='HTML'
        )

    except Exception as e:
        try:
            await loading_msg.delete()
        except:
            pass

        await message.answer(
            f"âŒ Unexpected error!\n\nInput: {channel_input}\nError: {str(e)}\n\nPlease try again.",
            reply_markup=get_admin_keyboard()
        )
    finally:
        await state.finish()


async def remove_channel_callback(callback_query: types.CallbackQuery):
    """Kanal o'chirish callback"""
    if not is_admin(callback_query.from_user.id):
        await callback_query.answer("âŒ Ruxsat yo'q!")
        return

    try:
        # Channel ID ni olish (callback_data: "remove_channel_@channelname" yoki "remove_channel_-100123")
        callback_parts = callback_query.data.split('_', 2)
        if len(callback_parts) >= 3:
            channel_id = callback_parts[2]
        else:
            await callback_query.answer("âŒ Xatolik: Channel ID topilmadi!")
            return

        db = Database()
        success = db.remove_channel(channel_id)

        if success:
            await callback_query.answer("âœ… Kanal o'chirildi!")
            # Yangilangan ro'yxatni ko'rsatish
            await channels_management_handler(callback_query.message)
        else:
            await callback_query.answer("âŒ Kanal topilmadi yoki xatolik yuz berdi!")

    except Exception as e:
        await callback_query.answer(f"âŒ Xatolik: {str(e)}")


