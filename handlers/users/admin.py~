from aiogram.dispatcher.filters import Command, Text
from data import config
from utils.db_api.database import Database
from keyboards.default.keyboards import get_admin_keyboard, get_main_menu
from datetime import datetime

# Google Sheets import
try:
    from sheets_integration import (
        save_user_with_qr_to_sheets,
        scan_qr_and_mark_attendance,
        get_sheets_url,
        clear_sheets_data
    )

    SHEETS_MODE = True
    print("âœ… Google Sheets integratsiyasi faol")
except ImportError:
    SHEETS_MODE = False
    print("âŒ Google Sheets integratsiyasi faol emas")


class AdminStates(StatesGroup):
    """Admin holatlari - yangilangan"""
    waiting_for_event_language = State()
    waiting_for_event_name = State()
    waiting_for_event_date = State()
    waiting_for_event_time = State()
    waiting_for_event_address = State()
    waiting_for_event_payment = State()
    qr_scanner_mode = State()
    waiting_for_channel_input = State()  # Faqat bitta kanal state


def is_admin(user_id):
    """Admin tekshirish"""
    return user_id in config.ADMINS


def get_language_selection_keyboard():
    """Til tanlash klaviaturasi"""
    keyboard = types.InlineKeyboardMarkup(row_width=3)
    keyboard.add(
        types.InlineKeyboardButton("ğŸ‡ºğŸ‡¿ O'zbek", callback_data='event_lang_uz'),
        types.InlineKeyboardButton("ğŸ‡·ğŸ‡º Ğ ÑƒÑÑĞºĞ¸Ğ¹", callback_data='event_lang_ru'),
        types.InlineKeyboardButton("ğŸ‡ºğŸ‡¸ English", callback_data='event_lang_en')
    )
    return keyboard


